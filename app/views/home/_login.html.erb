<%= form_tag(signin_path, method: :post) do %>
    <h1>Log In</h1>
	<%= label_tag %>
	<%= text_field_tag :username, nil, placeholder: 'Username' %>
	<br>
	<%= label_tag %>
	<%= password_field_tag :password, nil, placeholder: 'Password' %>
	<br>
	<%= submit_tag "Login", class: 'buttonStyle' %>
<% end %>