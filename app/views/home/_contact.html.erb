<%= form_for @contact, url: "/admin_update_contact_info_post", method: "post", :html => { :class => 'form-horizontal', multipart: true }  do |f| %>
	
	<%= f.text_field :phone_number, placeholder: "Phone Number" %><br />

	<%= f.text_field :email, placeholder: "Email" %><br />

	<%= f.text_field :street_address, placeholder: "Street Address" %><br />

	<%= f.text_field :address, placeholder: "City, State, Zipcode" %><br />
	
	<%= f.submit "Submit", :class => 'formbuttons'%>
<% end %>